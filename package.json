{
  "name": "handlebars-component-styleguide",
  "version": "2.2.1",
  "description": "Simple Handlebars live styleguide",
  "keywords": [
    "styleguide",
    "components",
    "handlebars",
    "patterns"
  ],
  "bin": {
    "styleguide": "bin/styleguide.js"
  },
  "dependencies": {
    "compression": "^1.4.4",
    "connect-livereload": "^0.6.1",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "livereload": "^0.9.1",
    "lodash": "^4.17.4",
    "minimist": "^1.1.1",
    "nodemon": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/Fcog/component-styleguide.git"
  },
  "bugs": {
    "url": "https://github.com/Fcog/component-styleguide/issues"
  },
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl",
    "url": "http://webpro.nl"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "^1.7.4"
  },
  "scripts": {
    "start": "node ./bin/styleguide.js",
    "watch": "nodemon --watch '**/*' --ext js,hbs,json",
    "prettier": "prettier --single-quote --write \"{bin,server}/**/*.js\""
  }
}
